    {{render '@quickfacts--orange'}}


    <section class="true-learning-community true-learning-department-cuminty">
      <div class="container">
        {{render '@blockquote--image-orange'}}
      </div>
    </section>

    {{render '@faculty-group'}}

    <!-- ENROLLMENT CTA -->
    {{ render '@enrollment-call-to-action' }}
    <!-- END CTA -->
  </main>
  <!-- End Main Content Body -->
  {{ render '@global-footer' }}
</div>